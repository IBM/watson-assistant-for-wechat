{
  "name": "watson-assistant-for-wechat",
  "version": "1.0.0",
  "description": "Helping developers building a chatbot for WeChat powered by Watson Assistant",
  "main": "./lib",
  "author": "Michael Lin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IBM/watson-assistant-for-wechat.git"
  },
  "files": [
    "lib",
    "README.md",
    "package.json",
    "yarn.lock"
  ],
  "xo": {
    "prettier": true,
    "rule": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^next"
        }
      ]
    }
  },
  "scripts": {
    "start": "node app.js",
    "lint": "xo",
    "test": "nyc --reporter=lcov --reporter=text-summary ava"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "body-parser-xml": "^1.1.0",
    "ibm-watson": "^5.4.0",
    "lodash.set": "^4.3.2",
    "sinon": "^9.0.0"
  },
  "devDependencies": {
    "ava": "^3.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-middleware-visualizer": "^0.1.3",
    "express-session": "^1.17.0",
    "lodash.get": "^4.4.2",
    "memorystore": "^1.6.2",
    "morgan": "^1.9.1",
    "node-weixin-message": "^0.1.1",
    "nyc": "^15.0.0",
    "xo": "^0.27.1"
  }
}
